evaluation:
  evaluator_models: []  # Empty means use generation models as evaluators
  tasks:
  - exact_model
  - binary_self
  temperature: 0.6  # Temperature for evaluation generation (0.0 = deterministic, higher = more creative)
  reasoning_effort: low  # Reasoning effort level: 'low', 'medium', 'high'
  max_tokens_exact: 2000  # Max tokens for exact model prediction responses
  max_tokens_binary: 1000  # Max tokens for binary self-identification responses
  max_tokens_judge: 10  # Max tokens for LLM judge responses
  request_delay: 1.0
  save_predictions: true
  predictions_dir: results/predictions_500
  append_predictions: true  # Append to existing predictions file by default

output_dir: results/500
plot_dir: results/plot_500
corpus_file: data/corpus-all-500.jsonl
